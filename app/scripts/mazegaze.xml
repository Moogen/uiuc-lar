<application>
<name>Maze Puzzle</name>
<dependencies>
<port>/root</port>
</dependencies>
<module>
	<name>icubmoddev</name>
	<parameters>--device grabber --subdevice dragonfly2 --width 320 --height 240 --video_type 1 --white_balance 0.464 0.757 --gain 0.233 --shutter 0.703 --name /icub/cam/left  --brightness 0 --DR2 --stamp --d 0 --sharpness 0.5 --hue 0.48 --gamma 0.4 --saturation 0.271 --framerate 60</parameters>
	<node>pc104</node>
	<tag>camera_left</tag>
</module>
<module>
	<name>icubmoddev</name>
	<parameters>--device grabber --subdevice dragonfly2 --width 320 --height 240 --video_type 1 --white_balance 0.464 0.757 --gain 0.233 --shutter 0.703 --name /icub/cam/right  --brightness 0 --DR2 --stamp --d 1 --sharpness 0.5 --hue 0.48 --gamma 0.4 --saturation 0.271 --framerate 60</parameters>
	<node>pc104</node>
	<tag>camera_right</tag>
</module>
<module>
	<name>yarpview</name>
	<parameters>--name /icub/view/left --x 0 --y 0 --synch</parameters>
	<node>johnny5</node>
	<tag>viewleft</tag>
</module>
<module>
	<name>yarpview</name>
	<parameters>--name /icub/view/right --x 320 --y 0 --synch</parameters>
	<node>johnny5</node>
	<tag>viewright</tag>
</module>
<module>
	<name>yarpview</name>
	<parameters>--name /maze/view:o --x 640 --y 0 --synch</parameters>
	<node>johnny5</node>
	<tag>mazeveiw_out</tag>
</module>
<module>
	<name>yarpview</name>
	<parameters>--name /maze/view:lo --x 960 --y 0 --synch</parameters>
	<node>johnny5</node>
	<tag>mazeview_homog</tag>
</module>
<module>
	<name>yarpview</name>
	<parameters>--name /maze/view:ro --x 1280 --y 0 --synch</parameters>
	<node>johnny5</node>
	<tag>mazeview_right</tag>
</module>
<connection>
	<from>/icub/cam/left</from>
	<to>/icub/view/left</to>
	<protocol>tcp</protocol>
</connection>
<connection>
	<from>/icub/cam/right</from>
	<to>/icub/view/right</to>
	<protocol>tcp</protocol>
</connection>
<connection>
	<from>/icub/cam/left</from>
	<to>/maze/img:l</to>
	<protocol>tcp</protocol>
</connection>
<connection>
	<from>/icub/cam/right</from>
	<to>/maze/img:r</to>
	<protocol>tcp</protocol>
</connection>
<connection>
	<from>/maze/img:o</from>
	<to>/maze/view:o</to>
	<protocol>tcp</protocol>
</connection>
<connection>
	<from>/maze/img:lo</from>
	<to>/maze/view:lo</to>
	<protocol>tcp</protocol>
</connection>
<connection>
	<from>/maze/img:ro</from>
	<to>/maze/view:ro</to>
	<protocol>tcp</protocol>
</connection>
</application>
